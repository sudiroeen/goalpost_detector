<?xml version="1.0" ?>
<launch>

   <node pkg="v9_goalpost_global" name="v9_goalpost_global_node" type="v9_goalpost_global_node" output="screen">
   </node>
   <node pkg="usb_cam" type="usb_cam_node" name="usb_cam_node" output="screen">
		  <param name="video_device" type="string" value="/dev/video0" />	
        <param name="pixel_format" type="string" value="yuyv" />
        <param name="camera_name" type="string" value="camera" />
        <param name="camera_info_url" type="string" value="file://$(find v9_goalpost_global)/config/lifecam_cinema.yaml" />
        <param name="image_width" type="int" value="640"/>
        <param name="image_height" type="int" value="480"/>
        <param name="autoexposure" type="bool" value="false"/>
        <!-- <param name="exposure" type="int" value="39"/> -->
        <!-- <param name="camera_info_url" type="string" value="file://$(find v9_goalpost_global)/config/c922_calib_data.yaml" /> -->
	</node>
</launch>

